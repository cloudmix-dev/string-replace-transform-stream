    "@types/node": "~20.10.5",
    "ava": "~6.0.1",
    "changeset": "~0.2.6",
    "husky": "~8.0.3",
    "tsx": "~4.7.0",
    "typescript": "~5.3.3",
    "vite": "~5.0.10",
    "vite-plugin-dts": "~3.7.0"
  }
